ejecutar:
    javac -cp "lib/*" -d bin src/uni/*.java

    Copy-Item -Recurse -Force src\META-INF\ -Destination bin\

    java --add-opens java.base/java.lang=ALL-UNNAMED -cp "bin;lib/*" uni.Test


comprobar conexión: 
    tnc localhost -port 1521       

PR4_PARTE2/
├── .vscode/
│   └── settings.json
├── bin/                          <-- Donde se generan los .class
│   ├── META-INF/
│   │   └── persistence.xml       <-- ¡IMPORTANTE! Este debe ir aquí
│   └── uni/
│       ├── Cliente.class
│       ├── Cuenta.class
│       ├── IBAN.class
│       ├── Oficina.class
│       ├── Operacion.class
│       └── Test.class
├── lib/                          <-- Librerías JAR necesarias
│   └── *.jar                     <-- (hibernate, openjpa, etc.)
├── src/                          <-- Código fuente
│   ├── META-INF/
│   │   └── persistence.xml       <-- El original (se copia al bin)
│   └── uni/
│       ├── Cliente.java
│       ├── Cuenta.java
│       ├── IBAN.java
│       ├── Oficina.java
│       ├── Operacion.java
│       └── Test.java
└── Readme


ALTER TRIGGER TRIGGER_VERIFICAR_TITULAR DISABLE;




DELETE FROM Operacion;
DELETE FROM Titular;
DELETE FROM Cuentas;
DELETE FROM Oficinas;
DELETE FROM Clientes;




