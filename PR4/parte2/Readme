ejecutar:
    javac -cp "lib/" -d bin src/uni/.java

    Copy-Item -Recurse -Force src\META-INF\ -Destination bin\

    java -cp "bin;lib/*" uni.Test


comprobar conexión: 
    tnc localhost -port 1521       

